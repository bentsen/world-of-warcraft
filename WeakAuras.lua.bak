["customText"] = "function()   \n    local function formatPrice(price, color)\n        if type(price) == \"string\" then\n            price = tonumber(price) or 0;\n        end\n        if price > 0 then\n            local cv = price % 100;\n            local sv = math.floor(price / 100) % 100;\n            local gv = math.floor(price / 10000);\n            local c = color and aura_env.colorize(color, cv) or cv;\n            local s = color and aura_env.colorize(color, sv) or sv;\n            local g = color and aura_env.colorize(color, gv) or gv;\n            if gv > 0 then\n                g = g .. \"|cffffff00g |r\";\n                if sv < 10 then\n                    s = \" \" .. s;\n                end\n                if cv < 10 then\n                    c = \" \" .. c;\n                end\n                s = s .. \"|cffb0b0b0s |r\";\n                c = c .. \"|cffd17c4bc |r\";\n            else\n                g = \"\"\n                if sv > 0 then\n                    s = s .. \"|cffb0b0b0s |r\";\n                    if cv < 10 then\n                        c = \" \" .. c;\n                    end\n                    c = c .. \"|cffd17c4bc |r\";\n                else\n                    s = \"\";\n                    if cv > 0 then\n                        c = c .. \"|cffd17c4bc |r\";\n                    else\n                        c = \"\";\n                    end\n                end\n            end\n            price = g .. (aura_env.config.duration.dispCopSilv and (s .. c) or \"\");\n        else\n            price = \"\";\n        end\n        return price;\n    end\n    \n    if aura_env.state.rarity then\n        local color = \"|c\" .. select(4, GetItemQualityColor(aura_env.state.rarity))\n        local name = aura_env.state.name;\n        local socket = aura_env.state.sock;\n        local tertiary = aura_env.state.tert;\n        local ilvl = aura_env.state.ilvl;\n        local isGear = aura_env.state.isGear;\n        local price = aura_env.state.price;\n        local priceV = price;\n        local priceB = formatPrice(price);\n        local isItem = aura_env.state.source == 3 or aura_env.state.source == 4;\n        \n        local appendLen = aura_env.state.appendLen or 0;\n        local append = aura_env.state.append or \"\";\n        if isItem and not isGear and not aura_env.state.isOptions then\n            append, appendLen = aura_env.GetInvCount(aura_env.state);\n        end\n        \n        local mLen = (aura_env.config.maxLength - appendLen) or 25;\n        if mLen < 0 then mLen = 0; end\n        if string.len(name) > mLen then\n            name = string.sub(name,1,mLen) .. \"...\";\n            --print(name);\n        end\n        name = color .. name .. append;\n        local stacks = tonumber(aura_env.state.stacks or 1);\n        local stackV = stacks;\n        if aura_env.state.source == 5 then --Rep\n            stacks = (tonumber(stacks) >= 0 and \"+\" or \"|cffFF0000\") .. tostring(stacks) .. (tonumber(stacks) >= 0 and \" \" or \"|r \");\n        else\n            if stacks > 1 then\n                stacks = stacks .. \"x \"\n            else\n                stacks = \"\"\n            end\n        end\n        price = \"\";\n        \n        if aura_env.config.showAH ~= 1 and \n        (WeakAuras.IsOptionsOpen() or (aura_env.config.showJunkAH or tonumber(aura_env.state.rarity) > 0)) and \n        aura_env.HasAuctionData then\n            if (aura_env.GetAuctionValue and aura_env.state.link) or \n            (WeakAuras.IsOptionsOpen() and isItem) then\n                local AH = (WeakAuras.IsOptionsOpen() and isItem) and math.floor(priceV * 1.5) or \n                (aura_env.GetAuctionValue(aura_env.state.link));\n                if aura_env.config.showAH == 3 and stackV > 1 then\n                    price = (AH and AH ~= \"\") and formatPrice(AH * stackV, aura_env.config.ahColor) or \"\";\n                    aura_env.state.AH = AH;\n                else\n                    price = (AH and AH ~= \"\") and formatPrice(AH, aura_env.config.ahColor) or \"\";\n                    aura_env.state.AH = AH;\n                end\n            end\n        end\n        if aura_env.config.showStackPrice ~= 3 or \n        (aura_env.config.showStackPrice == 3 and stackV == 1) then\n            if price ~= \"\" then\n                price = price .. \"\\n\"\n            end\n            price = price .. priceB;\n        end\n        if stackV > 1 and priceV > 0 and aura_env.config.showStackPrice ~= 2 then\n            if price ~= \"\" then\n                price = price .. \"\\n\"\n            end\n            price = price --[[.. \"|cff9d9d9d\" .. stackV .. \"|r \"]] .. formatPrice(stackV * priceV, aura_env.config.stackColor);\n        end\n        return stacks .. name ..                                        --Count and item name\n        (((socket and socket ~= \"\") or (tertiary and tertiary ~= \"\") or \n                (isGear and ilvl)) and \"\\n\" or \"\") ..                   --Newline if any secondary info\n        ((isGear and ilvl) and (\"|rilvl: \" .. tostring(ilvl) .. \" \") or \"\") .. --ilvl for gear\n        (tertiary or \"\") .. (socket and \" \" .. socket or \"\"),           --Socket/tertiary info\n        price, quantity, aura_env.state.source\n    end\nend",
