["custom"] = "function(allstates, event, _, subevent, _, sourceGUID, ...)\n    local s = allstates[\"\"] or {\n        show = false,\n        changed = false,\n        autoHide = true,\n        progressType = \"timed\",\n        duration = 60,\n        expirationTime = 0\n    }\n    \n    allstates[\"\"] = s\n    \n    if event == \"COMBAT_LOG_EVENT_UNFILTERED\" then\n        local spellId = select(8, ...)\n        \n        if sourceGUID == WeakAuras.myGUID then\n            if spellId == 16886 then \n                -- 16886 = Nature's Grace\n                \n                if subevent == \"SPELL_AURA_APPLIED\" or subevent == \"SPELL_AURA_REFRESH\" then\n                    local _, _, _, _, duration, expirationTime = WA_GetUnitBuff(\"player\", spellId)\n                    \n                    s.expirationTime = expirationTime - duration + 60\n                    \n                    --elseif subevent == \"SPELL_AURA_REMOVED\" then\n                end\n                \n                s.show = true\n                s.changed = true\n                \n                return true\n                \n            elseif (spellId == 48517 or spellId == 48518) and (subevent == \"SPELL_AURA_APPLIED\" or subevent == \"SPELL_AURA_REFRESH\") then\n                -- 48517 = Eclipse (Solar)\n                -- 48518 = Eclipse (Lunar)\n                \n                s.expirationTime = 0\n                s.show = false\n                s.changed = true\n                \n                return true\n            end\n        end\n    end\nend",
