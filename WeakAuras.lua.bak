       end\n                i = i + 1\n                --else\n                --return nil;\n                --end\n            end\n        end\n        \n        local function IsGear(type)   \n            return type == \"Armor\" or type == \"Weapon\";\n        end\n        \n        local function IsBanned(index, type)\n            --print(\"IsBanned\",link);\n            return aura_env.IsBanned(index, type);\n        end\n        if event == \"WA_LOOT_UPDATE_TEXT\" then\n            return true;\n        end\n        \n        if event == \"PLAYER_MONEY\" then\n            if aura_env.config.debug then print(\"PLAYER_MONEY\", ...); end\n            if aura_env.trackMoney == nil then\n                aura_env.trackMoney = GetMoney()\n            end\n            local dMoney = GetMoney() - aura_env.trackMoney\n            local cur = FindMoney();\n            if cur then\n                dMoney = dMoney + (allstates[cur].price or 0);\n                i = cur;\n            end\n            if dMoney > 0 then\n                local itemIcon = 133788\n                if dMoney > 99 then\n                    if dMoney > 9999 then\n                        itemIcon = 133784\n                    else\n                        itemIcon = 133786\n                    end \n                end\n                if GetDur(\"gold\") <= 0 then return; end\n                allstates[i] = {\n                    show = true,\n                    changed = true,\n                    progressType = \"timed\",\n                    duration = GetDur(\"gold\"),\n                    expirationTime = GetTime() + GetDur(\"gold\"),\n                    autoHide = true,\n                    stacks = 1,\n                    name = \"Money\",\n                    icon = itemIcon,\n                    price = dMoney,\n                    rarity = 1,\n                    prio = -1,\n                    source = 1,\n                }\n            end\n            aura_env.trackMoney = GetMoney