["custom"] = "aura_env.bannedItems = {}; --By item link\naura_env.bannedReps = {}; --By rep name\naura_env.bannedCurs = {}; --By currency ID\naura_env.factionInfo = {};\naura_env.curNameToID = {};\n\n----[[ For retail only\naura_env.repStrings = {\n    [\"enUS\"] = {\n        [\"incName\"] = \".*with ([%w %-',%(%):]+) increased by.*\",\n        [\"decName\"] = \".*with ([%w %-',%(%):]+) decreased by.*\",\n        [\"incVal\"] = \".*increased by (%d+)%.?\",\n        [\"decVal\"] = \".*decreased by (%d+)%.?\",\n        [\"expName\"] = \"([%w %-',%(%):]+) has gained %d+ experience.\",\n        [\"expVal\"] = \"[%w %-',%(%):]- has gained (%d+) experience.\",\n    },\n    \n    [\"deDE\"]  = {\n        [\"incName\"] = \".*der Fraktion '([%w %-',%(%):]+)' hat sich um.*\",\n        [\"decName\"] = \".*der Fraktion '([%w %-',%(%):]+)' hat sich um.*\",\n        [\"incVal\"] = \".-(%d+) verbessert%.?\",\n        [\"decVal\"] = \".-(%d+) verschlechtert%.?\",\n        [\"expName\"] = \"([%w %-',%(%):]-) hat %d+ Erfahrung erlangt.\",\n        [\"expVal\"] = \"[%w %-',%(%):]- hat (%d+) Erfahrung erlangt.\",\n    },\n    \n};\n--]]\n\naura_env.GetExpansion = function()\n    local full = GetBuildInfo();\n    local exp = tonumber(string.match(full, \"(%d*)%..*\") or 0);\n    return exp;\nend\n\naura_env.GetRepString = function(string)\n    if aura_env.GetExpansion() == 1 and (string == \"incName\" or string == \"decName\") then\n        return \"Your ([%a %-',%(%)%d:]+) reputation.*\";\n    end\n    local locale = GetLocale();\n    return aura_env.repStrings[locale] and aura_env.repStrings[locale][string] or \"\";\nend\n\naura_env.IsAddOnLoaded = function(addon)\n    local exp = aura_env.GetExpansion();\n    if exp < 5 then\n        return IsAddOnLoaded(addon);\n    else\n        return C_AddOns.IsAddOnLoaded(addon);\n    end\nend\n\naura_env.GetNumFactions = function()\n    local exp = aura_env.GetExpansion();\n    if exp < 5 then\n        return GetNumFactions();\n    else\n        return C_Reputation.GetNumFactions();\n    end\nend\n\naura_env.GetFactionInfo = function(index)\n    local exp = aura_env.GetExpansion();\n    if exp < 5 then\n        return GetFactionInfo(index);\n    else\n        return C_Reputation.GetFactionDataByIndex(index);\n    end\nend\n\nlocal function scanFactions()\n    if aura_env.GetExpansion() > 9 then\n        C_Reputation.ExpandAllFactionHeaders();\n    end\n    for factionIndex = 1, aura_env.GetNumFactions() do\n        local info;\n        if aura_env.GetExpansion() < 5 then\n            local name, description, standingId, bottomValue, topValue, earnedValue, atWarWith,\n            canToggleAtWar, isHeader, isCollapsed, hasRep, isWatched, isChild, factionID = GetFactionInfo(factionIndex);\n            if name then\n                info = {\n                    ID = factionIndex,\n                    factionID = factionID, \n                    description = description, \n                    standingID = standingId, \n                    bottomValue = bottomValue, \n                    topValue = topValue, \n                    earnedValue = earnedValue, \n                    atWarWith = atWarWith,\n                    canToggleAtWar = canToggleAtWar, \n                    isHeader = isHeader, \n                    isCollapsed = isCollapsed, \n                    hasRep = hasRep, \n                    isWatched = isWatched, \n                    isChild = isChild,\n                    name = name\n                }\n            end\n        else\n            \n            info = aura_env.GetFactionInfo(factionIndex);\n            if info then\n                info.ID = factionIndex;\n            end\n        end\n        if aura_env.GetExpansion() > 9 then\n            if info and info.factionID then\n                info.friendshipInfo = C_GossipInfo.GetFriendshipReputation(info.factionID);\n                info.majorFactionInfo = C_MajorFactions.GetMajorFactionData(info.factionID);\n            end\n        end\n        if info then\n            aura_env.factionInfo[info.name] = info;\n        end\n    end\nend\nif aura_env.config.debug and aura_env.IsAddOnLoaded(\"DevTool\") then\n    DevTool:AddData(aura_env.factionInfo, \"fInfo\");\nend\n\nscanFactions();\n--TODO: find way to identify max currency index\nlocal curMax = 1;\nfor currencyIndex = 1, 100000 do\n    local info = C_CurrencyInfo.GetCurrencyInfo(currencyIndex);\n    if info then\n        info.ID = currencyIndex;\n        aura_env.curNameToID[info.name] = info;\n        curMax = currencyIndex;\n    end\nend\naura_env.curNameToID[\"*maxValue\"] = curMax;\nif aura_env.config.debug and aura_env.IsAddOnLoaded(\"DevTool\") then\n    DevTool:AddData(aura_env.curNameToID, \"cInfo\");\nend\n\naura_env.GetFacID = function(name)\n    if not aura_env.factionInfo or not aura_env.factionInfo[name] then return nil; end\n    return aura_env.factionInfo[name].ID or nil;\nend\n\naura_env.GetCurID = function(name)\n    if not aura_env.curNameToID or not aura_env.curNameToID[name] then return nil; end\n    return aura_env.curNameToID[name].ID or nil;\nend\n\naura_env.GetItemStats = function(link)\n    if aura_env.GetExpansion() > 4 then\n        return C_Item.GetItemStats(link);\n    else\n        return GetItemStats(link);\n    end\nend\n\naura_env.FindInBags = function(link)\n    for bag = 0, NUM_BAG_SLOTS + 1 do\n        for slot = 1, C_Container.GetContainerNumSlots(bag) do\n            if(C_Container.GetContainerItemLink(bag, slot) == link) then\n                return bag, slot;\n            end\n        end\n    end\n    return nil;\nend\n\naura_env.IsBound = function(link)\n    local bag, slot = aura_env.FindInBags(link);\n    if bag then\n        local info = C_Container.GetContainerItemInfo(bag, slot);\n        if not info then return; end\n        local isBound = info.isBound;\n        return isBound;\n    end\n    return false;\nend\n\naura_env.IsBanned = function(index, type)\n    if not index then return false; end\n    if type == 1 then --Item\n        return aura_env.bannedItems[index];\n    elseif type == 2 then --Currency\n        return aura_env.bannedCurs[index];\n    elseif type == 3 then --Rep\n        return aura_env.bannedReps[index];\n    else \n        return false;\n    end\nend\n\naura_env.getFactionInfo = function(name)\n    if not aura_env.factionInfo then return nil; end\n    return aura_env.factionInfo[name];\nend\n\n\n\naura_env.colorize = function(colorArray, text)\n    local color = CreateColor(colorArray[1], colorArray[2], colorArray[3], colorArray[4])\n    return color:WrapTextInColorCode(text)\nend\n\naura_env.HasAuctionData = function()\n    if aura_env.config.showAH == 1 then return; end\n    local auctionAddon = aura_env.config.auctionAddon;\n    if auctionAddon == 1 then --Auctionator\n        return aura_env.IsAddOnLoaded(\"Auctionator\") and \n        Auctionator and Auctionator.API and Auctionator.API.v1 and\n        Auctionator.API.v1.GetAuctionPriceByItemLink;\n        \n    elseif auctionAddon == 2 then --TSM\n        local source = aura_env.config.tsmDataSource;\n        if source == 1 then --market\n            return aura_env.IsAddOnLoaded(\"TradeSkillMaster\") and TSM_API and TSM_API.GetCustomPriceValue;\n        elseif source == 2 then --minBuyout\n            return aura_env.IsAddOnLoaded(\"TradeSkillMaster\") and TSM_API and TSM_API.GetCustomPriceValue;\n        end\n        \n    end\n    return false;\nend\n\naura_env.GetAuctionValue = function(link)\n    if not aura_env.HasAuctionData() then return \"\"; end\n    local auctionAddon = aura_env.config.auctionAddon;\n    if auctionAddon == 1 then --Auctionator\n        return Auctionator.API.v1.GetAuctionPriceByItemLink(\"HypoWA\", link);\n        \n    elseif auctionAddon == 2 then --TSM\n        local source = aura_env.config.tsmDataSource;\n        local id = strmatch(link, \"item:(%d+)\") or 0;\n        local tsmID = \"i:\" .. id;\n        if id ~= 0 then\n            if source == 1 then --market\n                return TSM_API.GetCustomPriceValue(\"dbmarket\", tsmID);\n            elseif source == 2 then --minBuyout\n                return TSM_API.GetCustomPriceValue(\"dbminbuyout\", tsmID);\n            end\n        else \n            return 0;\n        end\n        \n    end\nend\n\n\naura_env.GetFactionStanding = function(facName)\n    scanFactions();\n    local info = aura_env.factionInfo[facName];\n    if not info or not info.ID then \n        scanFactions();\n        info = aura_env.factionInfo[facName];\n        if not info or not info.ID then return \"\"; end\n    end\n    local renownLevel = nil;\n    local rs = \" R: \";\n    local name, description, standingId, bottomValue, topValue, earnedValue, atWarWith,\n    canToggleAtWar, isHeader, isCollapsed, hasRep, isWatched, isChild, factionID = aura_env.GetFactionInfo(info.ID);\n    local majorFactionInfo = nil;\n    local friendshipInfo = nil;\n    if aura_env.GetExpansion() > 9 then\n        --print(\"test1\")\n        if (facName == \"Guild\" or facName == \"Gilde\") then --Special Case Guild rep\n            --print(\"test2\")\n            info = C_Reputation.GetFactionDataByID(1168);\n        end\n        bottomValue = info and info.currentReactionThreshold or 0;\n        topValue = info and info.nextReactionThreshold or 0;\n        earnedValue = info and info.currentStanding or 0;\n        name = info.name or \"\";\n        factionID = info.factionID;\n        topValue = info.nextReactionThreshold;\n        bottomValue = info.currentReactionThreshold;\n        earnedValue = info.currentStanding;\n        --print(\"0\",bottomValue,topValue,earnedValue)\n        \n        if facName ~= \"Inactive\" and facName ~= \"Other\" then\n            friendshipInfo = C_GossipInfo.GetFriendshipReputation(factionID);\n            majorFactionInfo = C_MajorFactions.GetMajorFactionData(factionID);\n        end\n        \n        \n        if friendshipInfo and friendshipInfo.friendshipFactionID and \n        friendshipInfo.friendshipFactionID ~= 0 then\n            bottomValue = friendshipInfo.reactionThreshold;\n            topValue = friendshipInfo.nextThreshold;\n            earnedValue = friendshipInfo.standing;\n            renownLevel = friendshipInfo.reaction;\n        end\n        \n        if majorFactionInfo and majorFactionInfo.renownLevelThreshold and \n        C_Reputation.IsMajorFaction(info.factionID)then\n            bottomValue = 0;\n            topValue = majorFactionInfo.renownLevelThreshold;\n            earnedValue = majorFactionInfo.renownReputationEarned;\n            renownLevel = majorFactionInfo.renownLevel;\n            --print(\"1\",bottomValue,topValue,earnedValue,renownLevel)\n        end\n        --print(\"2\",bottomValue,topValue,earnedValue,renownLevel)\n        if C_Reputation.IsFactionParagon(info.factionID) then\n            local currentValue, threshold, rewardQuestID, hasRewardPending, tooLowLevelForParagon =\n            C_Reputation.GetFactionParagonInfo(info.factionID);\n            renownLevel = math.floor(currentValue / threshold) - (hasRewardPending and 1 or 0);\n            bottomValue = 0;\n            earnedValue = math.floor(currentValue - (renownLevel * threshold));\n            topValue = threshold;\n            rs = \" P: \";\n        end\n    end  \n    \n    local ret = \" |r(\" .. tostring(earnedValue - bottomValue) .. \n    \" / \" .. tostring(topValue - bottomValue) .. \")\" .. \n    (renownLevel and (rs .. tostring(renownLevel)) or \"\");\n    local len = string.len(ret) - 3;\n    return ret, len;    \nend\n\naura_env.GetItemID = function(itemLink)\n    local itemID = itemLink:match(\"item:(%d*):\");\n    return itemID;\nend\n\nlocal function GetQuality(itemLink)\n    return tonumber(string.match(itemLink, \".-|A:Professions%-ChatIcon%-Quality%-Tier(%d)\") or 0);\nend\n\naura_env.GetStarLinks = function(itemLink)\n    local options = {};\n    local numStars = 0;\n    if string.match(itemLink, \"|A:Professions%-ChatIcon%-Quality%-Tier\") then\n        local base = tonumber(GetQuality(itemLink));\n        local baseID = tonumber(aura_env.GetItemID(itemLink));\n        for tier = 1, 3 do\n            options[tier] = (itemLink):gsub(\"Tier%d\", \"Tier\"..tier);\n            options[tier] = (options[tier]):gsub(\"item:(%d*):\", \"item:\" .. tostring(baseID + (tier - base)) .. \":\");\n            options[tier] = (options[tier]):gsub(\"38:(%d):\", \"38:\" .. tostring(tier) .. \":\");\n        end\n        numStars = 3;\n    else\n        options = itemLink;\n    end\n    \n    return options, numStars;\nend\n\naura_env.GetCounts = function(starLinks, numStars)\n    --print(\"Values:\",numStars,starLinks[1],starLinks[2], starLinks[3])\n    local suf = {\n        \":11:11::1|a\",\n        \":11:15::1|a\",\n        \":12:16::1|a\"\n    }\n    local pre = {\n        \"|r|cffd17c4b\",\n        \"|r|cffb0b0b0\",\n        \"|r|cffffff00\"\n    }\n    local appLen = 0;\n    local counts = \"\";\n    if numStars and numStars > 0 then\n        local hasParen = false;\n        for i = 1, numStars do\n            if starLinks[i] then\n                local count = C_Item.GetItemCount(starLinks[i]);\n                if count and count > 0 then\n                    if not hasParen then\n                        counts = counts .. \" |r(\";\n                        appLen = appLen + 2;\n                        hasParen = true;\n                    else\n                        counts = counts .. \" \";\n                        appLen = appLen + 1;\n                    end\n                    local newCount = pre[i]..tostring(count);\n                    appLen = appLen + string.len(newCount) - 11;\n                    newCount = newCount .. \"|A:Professions-ChatIcon-Quality-Tier\" .. tostring(i) .. suf[i];\n                    counts = counts .. newCount;\n                end\n            else\n                break;\n            end\n        end\n        if hasParen then\n            appLen = appLen + 1;\n            counts = counts .. \"|r)\";\n        end\n    else\n        local count = C_Item.GetItemCount(starLinks);\n        if count and count > 1 then\n            counts = \" |r(\" .. tostring(count) .. \")\";\n            appLen = string.len(counts) - 2;\n        end\n    end\n    return counts, appLen;\nend\n\naura_env.GetInvCount = function(state)\n    local countString = \"\";\n    local appendLen = 0;\n    if not state then return countString, appendLen; end\n    \n    if aura_env.config.invCount then\n        if state.link then\n            if aura_env.config.advCount and aura_env.GetExpansion() > 9 then\n                local starOps, numStars = aura_env.GetStarLinks(state.link);\n                local counts, appLen = aura_env.GetCounts(starOps, numStars);\n                if appLen and appLen > 0 then\n                    countString = counts;\n                    appendLen = appLen;\n                end\n            else\n                local count = C_Item.GetItemCount(state.link);\n                if count > 1 then\n                    countString = \" |r(\"..tostring(count)..\") \";\n                    appendLen = string.len(tostring(count)) + 2;\n                end\n            end\n            if not state.initialValue then\n                state.initialValue = countString;\n                state.numTries = 0;\n                --print(\"Setting initial Value\", state.initialValue);\n            end\n            --if state.fixed then print(\"Fixed\"); end\n            --if state.numTries and state.numTries > 5 then print(\"Failed\"); end\n            if not state.fixed and \n            (state.numTries and state.numTries <= 3) then\n                --print(state.stacks, state.initialValue);\n                if countString ~= state.initialValue then\n                    state.fixed = true;\n                    state.numTries = nil;\n                end\n                C_Timer.After(0.2, function()\n                        --print(\"Checking... \"..tostring(state.numTries));\n                        if not state or not state.numTries then return; end\n                        state.changed = true;\n                        state.numTries = (state.numTries or 3) + 1;\n                        WeakAuras.ScanEvents(\"WA_LOOT_UPDATE_TEXT\");\n                end);\n            end\n            \n        end\n    end\n    return countString, appendLen;\nend\n\n\nfor _,v in ipairs(aura_env.config.blacklist) do\n    if v and v.type == 1 and v.itemID and v.itemID ~= 0 then --Item\n        local _, link = GetItemInfo(v.itemID);\n        if link then\n            aura_env.bannedItems[link] = true;\n        end\n    elseif v and v.type == 2 and v.itemID then --Currency\n        aura_env.bannedCurs[v.itemID] = true;\n    elseif v and v.type == 3 and v.name then --Rep\n        aura_env.bannedReps[v.name] = true;\n    end\nend",
