["custom"] = "function(event, ...)\n    local arg1, arg2 = ...\n    \n    if \"OPTIONS\" == event or \"LWA_OPTIONS\" == event or (\"LWA_INIT\" == event and arg1) then\n        aura_env.ThrottledInit()\n        \n    elseif \"LWA_INIT\" == event then\n        aura_env.Init()\n        \n    elseif \"PLAYER_ENTERING_WORLD\" == event and (arg1 or arg2) then\n        aura_env.ThrottledInit()\n        \n        C_Timer.After(1, function()\n                WeakAuras.ScanEvents(\"LWA_INIT\", true)\n        end)\n    elseif \"LWA_UPDATE_BAR\" == event and arg1 then\n        aura_env.UpdateBar(...)\n        \n    elseif \"LWA_UPDATE_RESOURCES\" == event then\n        aura_env.UpdateResources()\n        \n    elseif \"UPDATE_SHAPESHIFT_FORM\" == event then\n        local formId = GetShapeshiftFormID()\n        \n        if aura_env.lastFormId ~= formId then\n            aura_env.lastFormId = formId\n            \n            WeakAuras.ScanEvents(\"LWA_UPDATE_SHAPESHIFT_FORM\")\n            \n            C_Timer.After(0.2, function()\n                    WeakAuras.ScanEvents(\"LWA_UPDATE_RESOURCES\")\n            end)\n        end\n    else\n        C_Timer.After(0.1, function()\n                WeakAuras.ScanEvents(\"LWA_UPDATE_RESOURCES\")\n        end)\n    end\n    \n    return true\nend",
["events"] = "OPTIONS,LWA_OPTIONS,LWA_INIT,LWA_UPDATE_RESOURCES,LWA_UPDATE_BAR,PLAYER_ENTERING_WORLD,PLAYER_SPECIALIZATION_CHANGED,PLAYER_TALENT_UPDATE,PLAYER_PVP_TALENT_UPDATE,PLAYER_LEVEL_UP,UNIT_PET:player,UNIT_ENTERED_VEHICLE:player,UNIT_EXITED_VEHICLE:player,UNIT_HEALTH:pet,CINEMATIC_STOP,STOP_MOVIE,CLIENT_SCENE_CLOSED,TRAIT_CONFIG_CREATED,TRAIT_CONFIG_UPDATED,TRAIT_TREE_CHANGED,PLAYER_LOSES_VEHICLE_DATA:player,PLAYER_CONTROL_GAINED,UPDATE_SHAPESHIFT_FORM,LWA_UPDATE_SHAPESHIFT_FORM",
